// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>

int main() {
    int charToInt(char c){
	int num = 0;
	num = c - '0';
	return num;
    }

    int sumofstr(char * c,int k){
        int sd = 0;
        for(int i=0;i<k;++i){
            if(c[i] >='0'&& c[i]<='9'){
            sd = sd + charToInt(c[i]);
            }
        }
        return sd;
    }


    char * A = "thi121s!!!is!!!Gre2132ta!!!!ifwih!!!213123";
    char * n = "!!!";
    int sum[100]={0};
    int cnt=0;

    while (strstr(A,n)){
        int k= strstr(A,n)-A;
        printf("S:%d\n",sumofstr(A,k));
        cnt = cnt+1;
        A = strstr(A,n) +3;
    }
    
    printf("S:%d\n",sumofstr(A,sizeof(A)));
}
