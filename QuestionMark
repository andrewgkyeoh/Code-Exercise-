#include <stdio.h> 
#include <string.h>

    int txe(char * str){
        int dc = 0;
        int tru = 0;
//       printf("D:%ld \n", strlen(str));
    
        for (int i =0;i<strlen(str);++i){
            int qc=0;
            if (str[i] >='0'&& str[i]<='9'){
                for (int j=i+1;j<strlen(str);++j){
                        if (str[j]=='?'){
                            ++qc;
                            continue;}
                        if (str[j] >='0'&& str[j]<='9'){
//                              printf("IF1 i:%d j:%d \n" , i, j);                            
                            if (str[i]-'0' +str[j]-'0'==10){
//                              printf("IF2 i:%d j:%d \n" , i, j);
                                if(qc!=3){
                                    printf("false");
                                    return 0;}
                                tru = 1; 
                                i = j-1;
                                break;}
                            else { // !=10
//                              printf("ignore2\n");
                                i=j;
                                break;}
                            break;}
                    if (j==strlen(str)-1){
                        printf("false");
                        return 0;}            
                    }
            }

        }
        if (tru){printf("true\n");}
        else {printf("false\n");}
    }

int main(void) { 
  // keep this function call here
  txe(coderbyteInternalStdinFunction(stdin));
  return 0;
    
}
